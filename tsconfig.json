{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node",
    "lib": [
      "es5",
      "es6",
      "es2015.promise"
    ],
    "outDir": "dist",
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*"
  ]
}